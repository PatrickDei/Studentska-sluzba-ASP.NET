@using StudentskaSluzba.Model
@model List<Professor>

@{ 
    ViewData["Title"] = "Popis profesora";
}

<partial name="_ProfessorFilter" />

<table class="table">
    <thead>
        <tr>
            <th scope="col">Ime</th>
            <th scope="col">Prezime</th>
            <th scope="col">Godina rođenja</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var s in Model)
        {
            <tr>
                <td><a asp-action="Details" asp-route-id="@s.ID">@s.FirstName</a></td>
                <td>@s.LastName</td>
                <td>@s.DateOfBirth.Year</td>
            </tr>
        }
            
        </tbody>
</table>